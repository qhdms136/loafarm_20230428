<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.loafarm.test.dao.TestMapper">
 	<select id="selectTestList" resultType="com.loafarm.test.model.Test">
 		SELECT
 			`id`
			, `loginId`
			, `password`
			, `nickname`
			, `email`
			, `createdAt`
